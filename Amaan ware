local a={main={enabled=true,aimlockkey="q",prediction=0.135,aimpart='HumanoidRootPart',notifications=true}}local b={box={Showbox=true,boxsize=Vector3.new(3.3,3,2.8),markercolor=Color3.fromRGB(144,248,144),markersize=UDim2.new(1,0,3,0)}}local c=Instance.new("Part",game.Workspace)local d=game.Players.LocalPlayer:GetMouse()function makemarker(e,f,g,h,i)local c=Instance.new("BillboardGui",e)c.Name="PRIVATE-REWRITE!"c.Adornee=f;c.Size=UDim2.new(h,i,h,i)c.AlwaysOnTop=true;local j=Instance.new("Frame",c)j.Size=b.box.markersize;j.BackgroundColor3=g;local k=Instance.new("UICorner",j)k.CornerRadius=UDim.new(50,25)return c end;local l;d.KeyDown:Connect(function(m)if m==a.main.aimlockkey then if a.main.enabled==true then a.main.enabled=false;if a.main.notifications==true then l=FindClosestUser()game.StarterGui:SetCore("SendNotification",{Title="ware أمان",Text="Unlocked | أمان ware;"})end else l=FindClosestUser()a.main.enabled=true;if a.main.notifications==true then game.StarterGui:SetCore("SendNotification",{Title="أمان ware",Text="Locked On | أمان ware ;  "..tostring(l.Character.Humanoid.DisplayName)})end end end end)local n=game.Players:GetPlayers()function noob(o)local p;repeat wait()until o.Character;local q=makemarker(guimain,o.Character:WaitForChild(a.main.aimpart),Color3.fromRGB(107,184,255),0.10,8)q.Name=o.Name;o.CharacterAdded:connect(function(r)q.Adornee=r:WaitForChild("HumanoidRootPart")end)end;for s=1,#n do if n[s]~=game.Players.LocalPlayer then noob(n[s])end end;game.Players.PlayerAdded:connect(function(t)noob(t)end)spawn(function()c.Anchored=true;c.CanCollide=false;c.Size=b.box.boxsize;if b.box.Showbox==true then c.Transparency=0.70 else c.Transparency=1 end;makemarker(c,c,b.box.markercolor,0.40,1)end)function FindClosestUser()local u;local v=300;for s,w in pairs(game.Players:GetPlayers())do if w~=game.Players.LocalPlayer and w.Character and w.Character:FindFirstChild("Humanoid")and w.Character.Humanoid.Health~=0 and w.Character:FindFirstChild("HumanoidRootPart")then local x=game:GetService"Workspace".CurrentCamera:WorldToViewportPoint(w.Character.PrimaryPart.Position)local y=(Vector2.new(x.X,x.Y)-Vector2.new(game.Players.LocalPlayer:GetMouse().X,game.Players.LocalPlayer:GetMouse().Y)).magnitude;if y<v then u=w;v=y end end end;return u end;game:GetService"RunService".Stepped:connect(function()if a.main.enabled and l.Character and l.Character:FindFirstChild("HumanoidRootPart")then c.CFrame=CFrame.new(l.Character[a.main.aimpart].Position+l.Character.UpperTorso.Velocity*a.main.prediction)else c.CFrame=CFrame.new(0,9999,0)end end)repeat wait()until game:IsLoaded()local z=getrawmetatable(game)local A=z.__namecall;setreadonly(z,false)z.__namecall=newcclosure(function(...)local B={...}if a.main.enabled and getnamecallmethod()=="FireServer"and B[2]=="UpdateMousePos"then B[3]=l.Character[a.main.aimpart].Position+l.Character[a.main.aimpart].Velocity*a.main.prediction;return A(unpack(B))end;return A(...)end)
